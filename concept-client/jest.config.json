{
  "testURL": "http://localhost/",
  "testMatch": ["**/?(*.)(test).ts?(x)"],
  "testPathIgnorePatterns": ["/node_modules/", "/target/"],
  "setupFiles": [
    "raf/polyfill",
    "<rootDir>/src/setupTest.ts"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "identity-obj-proxy"
  }
}
